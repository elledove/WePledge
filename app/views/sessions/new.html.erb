<h1> Welcome back! We missed you! Let's get you logged in. ;)</h1>
<p><i><%= @error %></i></p>

<%= form_tag('/login') do %>
<div><%= label_tag :username %></div>
<%= text_field_tag :username %>

<div><%= label_tag :password %></div>
<%= password_field_tag :password %>

<%= submit_tag "Login!" %>

<% end %>

<div>

<p><%= link_to "Login with your Github", "auth/github"  %></p>


</div>